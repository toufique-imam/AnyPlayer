<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_bottom_navbar"
    app:startDestination="@id/browseFragment">

    <fragment
        android:id="@+id/browseFragment"
        android:name="com.stream.jmxplayer.ui.fragment.BrowseFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_browse">
        <action
            android:id="@+id/action_browseFragment_to_streamFragment2"
            app:destination="@id/streamFragment" />

    </fragment>
    <fragment
        android:id="@+id/streamFragment"
        android:name="com.stream.jmxplayer.ui.fragment.StreamFragment"
        android:label="Stream Personal"
        tools:layout="@layout/fragment_stream">
        <action
            android:id="@+id/action_streamFragment_to_historyFragment2"
            app:destination="@id/historyFragment" />
        <!--        <action-->
        <!--            android:id="@+id/action_streamFragment_to_m3UDisplayFragment"-->
        <!--            app:destination="@id/m3UDisplayFragment" />-->
        <action
            android:id="@+id/action_streamFragment_to_m3uDisplayCategoryFragment"
            app:destination="@id/m3uDisplayCategoryFragment" />

    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.stream.jmxplayer.ui.fragment.HistoryFragment"
        android:label="@string/history"
        tools:layout="@layout/fragment_list_view">
        <action
            android:id="@+id/action_historyFragment_to_settingsFragment2"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.stream.jmxplayer.ui.fragment.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <!--    <fragment-->
    <!--        android:id="@+id/m3UDisplayFragment"-->
    <!--        android:name="com.stream.jmxplayer.ui.fragment.M3UDisplayFragment"-->
    <!--        android:label="M3U Playlist"-->
    <!--        tools:layout="@layout/fragment_list_view" />-->
    <fragment
        android:id="@+id/m3uDisplayCategoryFragment"
        android:name="com.stream.jmxplayer.ui.fragment.M3uDisplayCategoryFragment"
        android:label="M3U Playlist"
        tools:layout="@layout/fragment_m3u_display_category" />
    <fragment
        android:id="@+id/webviewFragment"
        android:name="com.stream.jmxplayer.ui.fragment.WebViewFragment"
        android:label="Browse Web"
        tools:layout="@layout/fragment_webview" />
</navigation>